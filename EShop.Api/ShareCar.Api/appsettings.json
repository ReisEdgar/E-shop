{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "JwtIssuerOptions": {
    "Issuer": "shareCarWebAPI",
    "Audience": "http://localhost:3000/",
    "RoleClaimName": "role",
    "IdClaimName": "id",
    "RoleClaimValue": "api_access"
  },
  "FacebookAuthSettings": {
    "AppAccessTokenUrl": "https://graph.facebook.com/oauth/access_token?client_id={AppId}&client_secret={AppSecret}&grant_type=client_credentials",
    "UserInfoUrl": "https://graph.facebook.com/v2.8/me?fields=id,email,first_name,last_name,name,gender,birthday,picture&access_token={FacebookAccessToken}",
    "DebugTokenUrl": "https://graph.facebook.com/debug_token?input_token={FacebookAccessToken}&access_token={AccessToken}"
  }
}
